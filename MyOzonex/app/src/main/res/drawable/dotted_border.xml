<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="15dp"
        android:bottom="15dp"
        android:left="15dp"
        android:right="15dp">

        <shape>
            <stroke
                android:color="@color/bordureGroupPortrait"
                android:dashWidth="10dp"
                android:dashGap="10dp"
                android:width="2dp"/>

            <padding
                android:left="30dp"
                android:top="30dp"
                android:right="30dp"
                android:bottom="30dp"/>

            <corners android:radius="10dp" />
        </shape>
    </item>
</layer-list>